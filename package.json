{
  "name": "fsd-crawler",
  "version": "1.0.10",
  "description": "Инструмент для анализа зависимостей в проекте и построения схем",
  "main": "index.js",
  "bin": {
    "fsd-crawler": "./index.js"
  },
  "files": [
    "bin",
    "dist",
    "index.js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "go build -o bin/fsd-analyzer",
    "build:all": "npm run build:darwin-amd64 && npm run build:darwin-arm64 && npm run build:linux-amd64 && npm run build:linux-arm64 && npm run build:windows-amd64 && npm run build:windows-arm64",
    "build:darwin-amd64": "GOOS=darwin GOARCH=amd64 go build -o bin/fsd-analyzer-darwin-amd64",
    "build:darwin-arm64": "GOOS=darwin GOARCH=arm64 go build -o bin/fsd-analyzer-darwin-arm64",
    "build:linux-amd64": "GOOS=linux GOARCH=amd64 go build -o bin/fsd-analyzer-linux-amd64",
    "build:linux-arm64": "GOOS=linux GOARCH=arm64 go build -o bin/fsd-analyzer-linux-arm64",
    "build:windows-amd64": "GOOS=windows GOARCH=amd64 go build -o bin/fsd-analyzer-windows-amd64.exe",
    "build:windows-arm64": "GOOS=windows GOARCH=arm64 go build -o bin/fsd-analyzer-windows-arm64.exe",
    "build:current": "npm run build",
    "prepublishOnly": "npm run build:all",
    "analyze": "node index.js",
    "test": "go test -short",
    "prepare": "husky"
  },
  "keywords": [
    "fsd",
    "feature-sliced",
    "architecture",
    "analyzer",
    "dependencies"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/falkomerr/fsd-crawler.git"
  },
  "bugs": {
    "url": "https://github.com/falkomerr/fsd-crawler/issues"
  },
  "homepage": "https://github.com/falkomerr/fsd-crawler#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "husky": "^9.1.7"
  }
}
